{
  "name": "eslint-config-ericbiewener",
  "private": true,
  "workspaces": [
    "./*"
  ],
  "license": "ISC",
  "devDependencies": {
    "husky": "1.3.1",
    "lerna": "3.11.0",
    "lint-staged": "8.1.3",
    "prettier-eslint-cli": "4.7.1"
  },
  "lint-staged": {
    "**/*.js": [
      "prettier-eslint --prettier-last --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
