{
  "name": "eslint-config-ericbiewener",
  "private": true,
  "workspaces": [
    "./*"
  ],
  "license": "ISC",
  "devDependencies": {
    "husky": "3.0.0",
    "lerna": "3.15.0",
    "lint-staged": "9.2.0",
    "prettier-eslint-cli": "5.0.0"
  },
  "lint-staged": {
    "**/*.js": [
      "prettier-eslint --prettier-last --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
