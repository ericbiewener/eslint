{
  "name": "eslint-config-ericbiewener",
  "private": true,
  "workspaces": [
    "./*"
  ],
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.6.2",
    "husky": "3.0.8",
    "lerna": "3.16.4",
    "lint-staged": "9.4.1",
    "prettier-eslint-cli": "5.0.0"
  },
  "lint-staged": {
    "**/*.js": [
      "prettier-eslint --prettier-last --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
